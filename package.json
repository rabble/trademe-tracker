{
  "name": "mivoy",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "npx --package=typescript tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "worker:dev": "cd worker && wrangler dev --config ../wrangler.worker.toml",
    "worker:deploy": "cd worker && wrangler deploy --config ../wrangler.worker.toml",
    "worker:test": "cd worker && npm test",
    "scrape": "cd worker && npm run scrape",
    "scrape:debug": "cd worker && npm run scrape:debug",
    "deploy": "npm run build && wrangler deploy --config wrangler.worker.toml",
    "deploy:pages": "npm run build && cd dist && npx wrangler pages deploy . --project-name=mivoy",
    "deploy:pages:prod": "npm run build && cd dist && npx wrangler pages deploy . --project-name=mivoy --branch=main --commit-dirty=true",
    "supabase:migration:new": "supabase migration new",
    "supabase:migration:apply": "node scripts/apply-migration.js",
    "supabase:db:reset": "node scripts/reset-database.js",
    "supabase:types": "supabase gen types typescript > src/lib/supabase/schema.ts"
  },
  "overrides": {
    "amqplib": {
      "node": ">=0.8"
    }
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.4",
    "@mendable/firecrawl-js": "^1.20.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@types/leaflet": "^1.9.8",
    "@types/lodash": "^4.17.15",
    "amqplib": "^0.10.5",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "hase": "^2.0.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "playwright-aws-lambda": "^0.11.0",
    "playwright-core": "^1.50.1",
    "puppeteer-core": "^24.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.2.0",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.2",
    "typecheck": "^0.1.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250224.0",
    "@jest/globals": "^29.7.0",
    "@playwright/browser-chromium": "^1.50.1",
    "@tailwindcss/postcss": "^4.0.14",
    "@tailwindcss/typography": "^0.5.16",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.5",
    "@types/puppeteer-core": "^5.4.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "js-cookie": "^3.0.5",
    "postcss": "^8.5.3",
    "postcss-nested": "^7.0.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0",
    "vite": "^5.1.0",
    "wrangler": "^4.0.0"
  },
  "description": "A web application that automatically tracks favorited properties from TradeMe, primarily residential and land listings.",
  "main": "postcss.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
